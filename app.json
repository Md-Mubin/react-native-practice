{
  "expo": {
    "name": "My Todo App",
    "slug": "my-todo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/todoApp.png",
    "scheme": "reactnattivetodolist",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/todoApp.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.mubin.todoApp",
      "edgeToEdgeEnabled": false,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": ""
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/todoApp.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "34357904-c7b5-45de-bb99-8a55007e212f"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/34357904-c7b5-45de-bb99-8a55007e212f"
    }
  }
}
